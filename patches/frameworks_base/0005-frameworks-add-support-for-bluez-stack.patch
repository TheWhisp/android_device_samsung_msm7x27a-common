+            if (SystemProperties.getBoolean("ro.qualcomm.bluetooth.nap", true)) {
+                setBluetoothTetheringNative(true, BluetoothPanProfileHandler.NAP_ROLE,
+                   BluetoothPanProfileHandler.NAP_BRIDGE);
