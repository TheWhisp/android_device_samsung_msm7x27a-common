+    /*package*/ String getObjectPathFromAddress(String address) {
+        String path = mAdapterProperties.getObjectPath();
+        if (path == null) {
+            Log.e(TAG, "Error: Object Path is null");
+            return null;
+        }
+        path = path + address.replace(":", "_");
+        return path;
+    }
