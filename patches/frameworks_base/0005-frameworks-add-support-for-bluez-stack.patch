+    private String getValidUtf8String ( String str)
+    {
+        int Char, i;
+        String temp;
+
+        if (str == null) {
+            return null;
+        }
+
+        for (i = 0; i < str.length(); i++) {
+            Char = str.codePointAt(i);
+            if ((Char > 0x10FFFF) ||
+                ((Char & 0xFFFFF800) == 0xD800) ||
+                ((Char >= 0xFDD0) && (Char <= 0xFDEF)) ||
+                ((Char & 0xFFFE) == 0xFFFE)) {
+                break;
+            }
+        }
+
+        if (i != str.length()) {
+            try {
+                temp = new String(str.getBytes(), 0, i, "UTF-8");
+                str = temp;
+            } catch (Exception e) {
+                Log.e(TAG, "Exception" + e);
+            }
+        }
+        return str;
+    }
